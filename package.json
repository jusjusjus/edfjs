{
  "name": "edfjs",
  "version": "0.4.0",
  "description": "A javascript implementation of the European Data Format.",
  "main": "src/index.js",
  "unpkg": "dist/edf.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint src/*.js test/*.js",
    "test": "mocha --reporter spec",
    "cypress": "cypress run",
    "build": "webpack --config ./config/webpack.config.js",
    "start": "npx http-server",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jusjusjus/edfjs.git"
  },
  "author": "Justus Schwabedal <jschwabedal@gmail.com> (https://jusjusjus.github.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jusjusjus/edfjs/issues"
  },
  "watch": {
    "lint": [
      "src/*js",
      "test/*js"
    ],
    "test": [
      "src/*js",
      "test/*js"
    ],
    "build": "src/*js"
  },
  "homepage": "https://github.com/jusjusjus/edfjs#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-almost": "^1.0.1",
    "chai-datetime": "^1.6.0",
    "cypress": "^4.7.0",
    "cypress-file-upload": "^4.0.7",
    "eslint": "^7.1.0",
    "filereader": "^0.10.3",
    "fs": "0.0.1-security",
    "http-server": "^14.1.1",
    "https": "^1.0.0",
    "mocha": "^10.2.0",
    "npm-watch": "^0.11.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
